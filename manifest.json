{
    "manifest_version": 2,
    "name": "Google Slides",
    "version": "0.1.1",
    "description": "",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["app.js"]
    }],
    "permissions": [
        "*://*/*",
        "*://localhost/*",
        "tabs",
        "webNavigation"
    ]
}
